<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="MomondoWSSoap" />
            </basicHttpBinding>
            <customBinding>
                <binding name="MomondoWSSoap12">
                    <textMessageEncoding messageVersion="Soap12" />
                    <httpTransport />
                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://www.momondo.com/Momondo.asmx" binding="basicHttpBinding"
                bindingConfiguration="MomondoWSSoap" contract="MomondoService.MomondoWSSoap"
                name="MomondoWSSoap" />
        </client>
    </system.serviceModel>
</configuration>